[submodule ".registry"]
	path = .registry
	url = https://github.com/r-universe-org/cran-to-git
	shallow = true
	branch = HEAD
[submodule "geniusr"]
	path = geniusr
	url = https://github.com/ewenme/geniusr
	shallow = true
	registered = true
	oncran = true
[submodule "ghibli"]
	path = ghibli
	url = https://github.com/ewenme/ghibli
	shallow = true
	registered = true
	oncran = true
